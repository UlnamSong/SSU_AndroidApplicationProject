        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="6b00b950e8f4c66092f9c93b01ad9722" />

        <activity android:name="com.example.ulnamsong.linemappractice.LogoActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

