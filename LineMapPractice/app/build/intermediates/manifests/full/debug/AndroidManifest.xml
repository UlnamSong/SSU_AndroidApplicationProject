        <activity
            android:name="com.example.ulnamsong.linemappractice.TimatableActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />

        <receiver android:name="com.example.ulnamsong.linemappractice.appNetwork" >
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
    </application>

</manifest>